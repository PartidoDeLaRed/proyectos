{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "plugins": {
    "complete_strings": {},
    "node": {},
    "modules": {},
    "es_modules": {},
    "react": {},
    "node-express": {},
    "doc_comment": {
      "fullDocs": true
    }
  }
}
